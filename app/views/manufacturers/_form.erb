<%= form_with model: @manufacturer, local: true do |f| %>
  <%= render 'shared/error_messages', object: f.object %>

  <%= f.label :name, t('activerecord.attributes.manufacturer.name') %>
  <%= f.text_field :name, class: 'form-control' %>

  <%= f.submit t('views.actions.send'), class: 'btn btn-primary' %>
<% end %>
